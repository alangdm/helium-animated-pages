{
  "name": "helium-animated-pages",
  "version": "1.0.1",
  "description": "A light spiritual succesor to neon-animated-pages using only css animations",
  "main": "helium-animated-pages.js",
  "module": "helium-animated-pages.js",
  "repository": "https://github.com/alangdm/helium-animated-pages",
  "author": "alangdm",
  "license": "BSD-3-Clause",
  "dependencies": {
    "lit-element": "^2.2.1"
  },
  "devDependencies": {
    "@webcomponents/webcomponentsjs": "^2.4.1",
    "api-viewer-element": "^0.3.7",
    "es-dev-server": "^1.36.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-lit": "^1.2.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-wc": "^1.2.0",
    "husky": "^4.0.7",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "web-component-analyzer": "^1.0.1"
  },
  "scripts": {
